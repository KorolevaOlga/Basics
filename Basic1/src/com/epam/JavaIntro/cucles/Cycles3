package com.epam.JavaIntro.cucles;

//Найти сумму квадратов первых ста чисел

public class Main {

    public static void main(String[] args) {

        int sum = 0;
        for (int i = 0; i <= 100; i++) {
            sum += (int) Math.pow(i, 2);
        }
        System.out.println(sum);
    }
}

